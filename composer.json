{
    "name": "nailsapp/module-admin",
    "keywords": ["Nails", "Shed Collective", "CodeIgniter", "Admin Module"],
    "homepage": "http://nailsapp.co.uk/modules/admin",
    "description": "This is the \"Admin\" module for Nails.",
    "license": "MIT",
    "authors":
    [
        {
            "name": "Pablo de la Pena",
            "email": "p@nailsapp.co.uk",
            "homepage": "http://nailsapp.co.uk",
            "role": "Developer"
        },
        {
            "name": "Gary Duncan",
            "email": "g@nailsapp.co.uk",
            "homepage": "http://nailsapp.co.uk",
            "role": "Developer"
        }
    ],
    "support":
    {
        "email": "hello@nailsapp.co.uk"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require":
    {
        "nailsapp/common": "dev-develop",
        "nailsapp/module-auth": "dev-develop",
        "nailsapp/module-api": "dev-develop",
        "nailsapp/module-cdn": "dev-develop"
    },
    "require-dev":
    {
        "phpunit/phpunit": "4.*"
    },
    "autoload":
    {
        "psr-4": {"Nails\\Admin\\": "src/"}
    },
    "extra":
    {
        "nails" :
        {
            "moduleName": "admin",
            "type": "module",
            "namespace": "Nails\\Admin\\",
            "data": {
                "nailsapp/module-cdn": {
                    "permitted-image-dimensions": [
                        "30x30",
                        "35x35"
                    ]
                }
            }

        }
    }
