{
    "name": "nails/module-admin",
    "keywords": [
        "Nails",
        "Shed Collective",
        "CodeIgniter",
        "Admin Module"
    ],
    "homepage": "http://nailsapp.co.uk/modules/admin",
    "description": "This is the \"Admin\" module for Nails.",
    "license": "MIT",
    "authors": [
        {
            "name": "Pablo de la Pena",
            "email": "p@nailsapp.co.uk",
            "homepage": "http://nailsapp.co.uk",
            "role": "Developer"
        },
        {
            "name": "Gary Duncan",
            "email": "g@nailsapp.co.uk",
            "homepage": "http://nailsapp.co.uk",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "hello@nailsapp.co.uk"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "nails/common": "dev-feature/pre-new-admin",
        "nails/module-api": "dev-feature/pre-new-admin",
        "nails/module-auth": "dev-feature/pre-new-admin",
        "nails/module-cdn": "dev-feature/pre-new-admin",
        "nails/module-console": "dev-feature/pre-new-admin",
        "nails/module-cron": "dev-feature/pre-new-admin"
    },
    "require-dev": {
        "phpunit/phpunit": "9.*",
        "phpstan/phpstan": "1.*"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "analyse": "./vendor/bin/phpstan analyse -c .phpstan/config.neon"
    },
    "autoload": {
        "psr-4": {
            "Nails\\Admin\\": "src/"
        }
    },
    "extra": {
        "nails": {
            "moduleName": "admin",
            "type": "module",
            "namespace": "Nails\\Admin\\",
            "data": {
                "nails/module-api": {
                    "namespace": "admin"
                },
                "nails/module-cdn": {
                    "permitted-image-dimensions": [
                        "30x30",
                        "35x35"
                    ]
                }
            }
        }
    }
}
